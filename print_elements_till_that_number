import java.util.Scanner;

public class series
 {
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the nth term: ");
        int n = sc.nextInt();
        for(int i = 0 ; i<=n ; i++)
        {
            System.out.println(i);
        }
    }
}
